  "scripts": {
    "start": ". work-around-sherpa-assets.sh && webpack serve --config ./config/webpack.local.js",
    "start:mf:local": ". work-around-sherpa-assets.sh && webpack serve --config ./config/webpack.dev.js",
    "build": ". work-around-sherpa-assets.sh && . env-check.sh && tsc && webpack --config ./config/webpack.$REACT_APP_ENV.js",
    "test": "jest --coverage=true",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:sass": "stylelint **/*.scss",
    "pretty": "prettier --write .",
    "pretty-quick": "pretty-quick --staged",
    "prettier:list": "prettier --list-different \"./{e2e,src}/**/*.{scss,ts}\" || ECHO .",
    "prettier:write": "prettier --write \"./{e2e,src}/**/*.{ts}\"",
    "prettier:write:sass": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "prettier:all": "prettier \"**/*.{ts,js,json,html,scss,md}\" --write",
    "prettier": "prettier \"**/*.{ts,js,json,html,scss,md}\" --check",
    "customScript": "echo customScript not necessary at the moment!",
    "sonar-local": "npm run test && sonar-scanner -Dsonar.projectKey=1f029bfd795e01f0d4cfe44deb39143c47faf381 -Dsonar.organization=$SONAR_ORGANIZATION -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN"
  },
