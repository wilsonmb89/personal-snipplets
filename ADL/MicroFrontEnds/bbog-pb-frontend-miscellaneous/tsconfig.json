{
  "compilerOptions": {
    "baseUrl": ".",
    "noEmit": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@2fa": ["src/workflows/two-factor-authentication/index"],
      "@notification-modal": ["src/components/core/notification/index"],
      "@sherpa-react/*": ["node_modules/@npm-bbta/bbog-dig-dt-webcomponents-lib/dist/*"],
      "@components/*": ["src/components/*"],
      "@store/*": ["src/store/*"],
      "@test-utils/*": ["src/test-utils/*"],
      "@utils/*": ["src/utils/*"],
      "@constants/*": ["src/constants/*"]
    }
  }
}
